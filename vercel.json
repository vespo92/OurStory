{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun run build",
  "devCommand": "bun run dev",
  "installCommand": "bun install",
  "framework": null,
  "outputDirectory": "apps/timeline/.next",
  "builds": [
    {
      "src": "apps/timeline/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/timeline/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "claude/setup-turbo-repo-01DuyYrCnYY23qhKBCkHdAaA": true
    }
  }
}
